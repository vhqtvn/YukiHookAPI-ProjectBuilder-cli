{
  "name": "yukihookapi-projectbuilder",
  "productName": "YukiHookAPI Project Builder",
  "version": "1.0.2",
  "description": "A Xposed Project Builder by YukiHookAPI",
  "main": "cli.js",
  "bin": {
    "yukihookapi-cli": "./cli.js"
  },
  "scripts": {
    "start": "node cli.js"
  },
  "author": "HighCapable",
  "license": "AGPL-3.0",
  "dependencies": {
    "axios": "^0.27.2",
    "dayjs": "^1.11.3",
    "decompress-zip": "^0.3.3",
    "fs-extra": "^10.1.0",
    "inquirer": "^8.2.7",
    "xml2js": "^0.6.2"
  },
  "pkg": {
    "scripts": "src/**/*.js",
    "assets": [
      "public/**/*"
    ],
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-win-x64"
    ],
    "outputPath": "dist"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  }
}
